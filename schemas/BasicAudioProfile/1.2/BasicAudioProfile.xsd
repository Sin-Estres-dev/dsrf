<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://ddex.net/xml/dsrf/3/20180807" xmlns:avs="http://ddex.net/xml/avs/avs" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:dsrf-ba="http://ddex.net/xml/dsrf/3/20180807" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:import namespace="http://ddex.net/xml/avs/avs" schemaLocation="../../avs/current/avs.xsd"/>
